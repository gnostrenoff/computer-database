import java.util.Scanner;

public class Listener {

	public static void main(String[] args) {
		
		Scanner scanIn = new Scanner(System.in);
		
		System.out.println("what do you want to do ?\n");
		System.out.println("\t create (c)\n");
		System.out.println("\t read (r)\n");
		System.out.println("\t update (u)\n");
		System.out.println("\t delete (d)\n");
		System.out.println("\t exit (e)\n");
		
		
	    String input = scanIn.nextLine();
	    
	    switch(input){
	    case "create":
	    	break;
	    case "create":
	    	break;
	    case "create":
	    	break;
	    case "create":
	    	break;
	    case "create":
	    	break;
	    	default:
	    		exit(1);
	    	
	    }

	       scanIn.close();            

	}

}
